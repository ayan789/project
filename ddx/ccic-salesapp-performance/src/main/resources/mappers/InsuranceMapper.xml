	<select id="pendingEffectFeeQuery" parameterType="java.util.HashMap"
		resultType="java.math.BigDecimal">
		<!-- SELECT SUM(IFNULL(S.TOTAL_C1FEE_LIMIT,0))
		FROM T_INSURANCE_APPLICATION
		S,T_INSURANCE_ORDER TIO
		WHERE S.INSURANCE_ORDER_ID = TIO.ID
		AND
		TIO.INSURANCE_ORDER_NO LIKE 'M%'
		AND S.CHANNEL_CODE='2401'
		<if test='handlerCode!=null '>
			AND S.OPERATOR_EMPNO=#{handlerCode}
		</if>
		AND S.AGENT_CODE=#{agentCode}
		AND DATE_FORMAT(S.POLICY_CREATE_DATE,'Y%-m%-d%')=
		DATE_FORMAT(now(),'Y%-m%-d%') -->
		
		SELECT ifnull(sum(TOTAL_C1FEE_LIMIT),0) from t_vehicle_order TIO where TIO.AGENT_CODE=#{agentCode}
		and TIO.ID in (
		SELECT S.order_id from t_vehicle_insurance_application S where (POLICY_NO !='' and POLICY_NO is not null)
		and POLICY_NO in (
		SELECT POLICY_NO from t_vehicle_policy where DATE_FORMAT(create_time,'%Y-%m-%d')=DATE_FORMAT(now(),'%Y-%m-%d')
		)
		)
		
	</select>

